{
    "name": "postitapp",
    "version": "1.0.0",
    "description": "PostItApp is a simple application that allows friends and colleagues create groups for notifications.",
    "main": "webpack.confiig.js",
    "licence": "MIT",
    "engines": {
        "node": "6.9.4"
    },
    "scripts": {
        "start": "webpack --config webpack.prod.js && babel-node ./server/server.js",
        "test": "NODE_ENV=test nyc --reporter=text --reporter=html --reporter=lcov mocha ./server/__test__ --timeout 20000 --compilers js:babel-register && npm run coverage-report",
        "client_test": "jest ./client/__tests__",
        "coverage-report": "cat ./coverage/lcov.info | coveralls",
        "client": "webpack --config webpack.dev.js && babel-node ./server/server.js",
        "server": "babel-watch ./dist/server.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/QUDUSKUNLE/Post-it-app"
    },
    "author": "Qudus YEKEEN",
    "license": "ISC",
    "jest": {
        "roots": [
            "<rootDir>/client/__tests__",
            "<rootDir>/node_modules/babel-polyfill"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "collectCoverage": true,
        "coveragePathIgnorePatterns": [
            "<rootDir>/test-env.js"
        ]
